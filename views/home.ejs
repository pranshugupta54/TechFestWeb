<%- include('partials/header'); -%>

<h1>Home</h1>
<p> <%= startingContent %> </p>
<br>
<% events.forEach(function(event){ %>
<h4> <%= event.title %> </h4>
<% if(event.about.length > 100){%> 
<p> <%= event.about.substring(0,100)+ "... " %><a href="/events/<%=event.code%> ">Read More</a></p>
<%} else{ %>
    <%= event.about %> 
<% } %>   
<% }) %> 

<%- include('partials/footer'); -%>