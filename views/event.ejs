<%- include('partials/header'); -%>

<div class="post-content">
    <div class="post-text">
        <h1 class="heading"><%= eventFull.title %></h1>
        <div class="date">
            <li>&#x1f4c5; <%= eventFull.date %></li>
            <li>&#128337; <%= eventFull.time %></li>
            <li>&#x1F4CD; <%= eventFull.venue %></li>
        </div>
        <form action="/register" method="get"><button type="submit" >REGISTER</button></form>
        
    </div>
<!-- <img src="/media/cb1811.png" alt=""> -->
<img src="<%= eventFull.img %>" alt="">
</div>

<h1 class="heading" style="text-align:center; font-size: 50px;">INFORMATION</h1>

<h2 class="heads">ABOUT</h2>
<p> <%= eventFull.about %>  </p>

<h2 class="heads">RULES</h2>
<p><a class="no-dec" href="<%= eventFull.rules %>" target="_blank">Click here to view rules </a></p>

<h2 class="heads">TEAM SIZE</h2>
<p><%= eventFull.size %></p>

<h2 class="heads">PRIZE</h2>
<p>Cash prize worth <%= eventFull.prize %></p>

<h2 class="heads">CONTACT US</h2>
<p><%= eventFull.contact %></p>

<%- include('partials/footer'); -%>