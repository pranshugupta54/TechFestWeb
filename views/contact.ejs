<%- include('partials/header'); -%>

<h1 class="heading">Contact Us</h1>
<p> <%= startingContent %> </p>

<form class="contact_form" action="contact" method="post">
    <label for="Name">Name</label>
    <input type="text" name="Name" placeholder="Your Name" required autocomplete="off" autofocus>
    <label for="email">Email</label>
    <input type="email" name="email" placeholder="youremail@mail.com" required autocomplete="off">
    <label for="sub">Subject</label>
    <input type="text" name="sub" placeholder="How can we help?" required autocomplete="off">
    <label for="message">Message</label>
    <textarea name="message" cols="20" rows="5" placeholder="" required></textarea>
    <button type="submit">Send</button>
</form>

<%- include('partials/footer'); -%>